{
  "name": "paytabs-sdk",
  "version": "1.2.0",
  "description": "A node package for Paytabs integration",
  "keywords": [
    "paytabs",
    "api",
    "payment processing",
    "typescript"
  ],
  "homepage": "https://github.com/nostva/paytabs-sdk",
  "bugs": {
    "url": "https://github.com/nostva/paytabs-sdk/issues"
  },
  "main": "dist/index.js",
  "author": "Mohamed Saeed <mohamed.saeed.bakit@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nostva/paytabs-sdk.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "build": "tsup",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.28.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "axios": "^1.8.3"
  }
}
